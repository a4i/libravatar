/var/log/libravatar/access-www.log {
     create 644 www-data www-data
     rotate 12
     weekly
     compress
     missingok
}

/var/log/libravatar/error-www.log {
     create 644 www-data www-data
     rotate 12
     weekly
     compress
     missingok
}

/var/log/libravatar/workers.log {
     create 644 libravatar-img root
     rotate 12
     weekly
     compress
     missingok
}
